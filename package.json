{
  "name": "modulus",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "postinstall": "node scripts/fix-magic-bytes.js",
    "typecheck": "node scripts/run-tsc.js --noEmit",
    "test:risk": "npm run build:risk && node --test dist/__tests__/riskEngine.test.js",
    "build:risk": "node scripts/run-tsc.js -p tsconfig.json",
    "test:db": "node --test src/core/database/__tests__/mysqlPool.test.js",
    "test:reliability": "npm run test:db && npm run test:risk"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "mysql2": "^3.16.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
